    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "svelte": "^3.34.0",
    "svelte-check": "^2.2.6",
    "svelte-fullcalendar": "^1.1.1",
